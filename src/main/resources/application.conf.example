quad-store {
    query-url = "http://localhost:8081/bigdata/namespace/kb/sparql"
    query-url = "${?MMS5_STORE_QUERY}"
    update-url = "http://localhost:8081/bigdata/namespace/kb/sparql"
    update-url = "${?MMS5_STORE_UPDATE}"
}

mms {
    load-service {
        url = "https://mms-domain/loader"
        url = ${?MMS5_LOAD_SERVICE_URL}
    }

    quad-store {
        url = "https://quad-store-domain/sparql"
        url = ${?MMS5_QUAD_STORE_URL}
        query-url = ""
        query-url = ${?MMS5_QUERY_URL}
        graph-store-protocol-url = "https://quad-store-domain/sparql/gsp"
        graph-store-protocol-url = ${?MMS5_GRAPH_STORE_PROTOCOL_URL}
    }
}

ktor {
    deployment {
        port = 8080
    }

    application {
        modules = [ org.openmbee.mms5.ApplicationKt.module ]
    }

    development = true
}

jwt {
    domain = "https://jwt-provider-domain/"
    domain = ${?JWT_DOMAIN}
    audience = "jwt-audience"
    audience = ${?JWT_AUDIENCE}
    realm = "MMS5 Microservices"
    realm = ${?JWT_REALM}
    secret = "test1234"
    secret = ${?JWT_SECRET}
}
